{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "createdb": "createdb estl && createdb estl-test",
    "setup": "cat ./src/database/sql/employee/create.sql | psql estl && cat ./src/database/sql/employee/create.sql | psql estl-test",
    "start": "tsc && node dist/server.js",
    "start:watch": "nodemon",
    "test": "NODE_ENV=test jest --runInBand --verbose"
  },
  "dependencies": {
    "csv-parse": "^4.16.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "pg-promise": "^10.10.2",
    "ts-node": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/multer": "^1.4.6",
    "@types/pg-promise": "^5.4.3",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
